   <element abstract="false" id="uk-ref_linkPart_Year" name="Year" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Year in which reference material is published.  Format is CCYY.</documentation>
      </annotation>
   </element>
   <element abstract="false" id="uk-ref_linkPart_Schedule" name="Schedule" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Used to represent Schedules in legislation or other rulings.  Main UK example is the Schedules in the Companies Act.</documentation>
      </annotation>
   </element>
   <element abstract="false" id="uk-ref_linkPart_Part" name="Part" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Used to represent items broken into Parts as well as other sections.  A UK example is the Parts in the Companies Act.</documentation>
      </annotation>
   </element>
   <element abstract="false" id="uk-ref_linkPart_ISOName" name="ISOName" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Reference to specific ISO publication, excluding document number</documentation>
      </annotation>
   </element>
   <element abstract="false" id="uk-ref_linkPart_Code" name="Code" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Code used in ISO publication to represent data</documentation>
      </annotation>
   </element>
   <element abstract="false" id="uk-ref_linkPart_AlternativeCode" name="AlternativeCode" nillable="false" substitutionGroup="link:part" type="string">
      <annotation>
         <documentation>Alternative code used in ISO publication to represent data</documentation>
      </annotation>
