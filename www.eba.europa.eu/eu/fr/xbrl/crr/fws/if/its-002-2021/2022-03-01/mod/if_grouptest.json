{
    "dimensions": {
        "entity": "$entityID",
        "language": "$baseLanguage",
        "period": "$refPeriod@end"
    },
    "documentInfo": {
        "documentType": "https://xbrl.org/2021/xbrl-csv",
        "extends": [
            "http://www.eba.europa.eu/eu/fr/xbrl/ext/2020/FilingIndicators.json",
            "http://www.eba.europa.eu/eu/fr/xbrl/ext/2020/FootNotes.json",
            "../tab/i_11.01/i_11.01.json",
            "../tab/i_11.02/i_11.02.json",
            "../tab/i_11.03/i_11.03.json"
        ],
        "features": {
            "xbrl:allowedDuplicates": "complete"
        },
        "final": {
            "dimensions": true,
            "linkGroups": true,
            "linkTypes": true,
            "namespaces": true,
            "parameters": true,
            "tableTemplates": true,
            "tables": true,
            "taxonomy": true
        },
        "linkGroups": {
            "_": "http://www.xbrl.org/2003/role/link"
        },
        "linkTypes": {
            "footnote": "http://www.xbrl.org/2003/arcrole/fact-footnote"
        },
        "namespaces": {
            "eba": "http://www.eba.europa.eu",
            "iso4217": "http://www.xbrl.org/2003/iso4217",
            "lei": "http://standards.iso.org/iso/17442",
            "rs": "https://eurofiling.info/eu/rs",
            "xbrl": "https://xbrl.org/2021"
        },
        "taxonomy": [
            "http://www.eba.europa.eu/eu/fr/xbrl/crr/fws/if/its-002-2021/2022-03-01/mod/if_grouptest.xsd"
        ]
    },
    "parameterURL": "parameters.csv",
    "parameters": {
        "baseLanguage": "en"
    },
    "tables": {
        "tFI": {
            "template": "FilingIndicators",
            "url": "FilingIndicators.csv"
        },
        "tFootNotes": {
            "optional": true,
            "template": "FootNotes",
            "url": "FootNotes.csv"
        },
        "tI_11-01": {
            "optional": true,
            "template": "I_11-01",
            "url": "i_11.01.csv"
        },
        "tI_11-02": {
            "optional": true,
            "template": "I_11-02",
            "url": "i_11.02.csv"
        },
        "tI_11-03": {
            "optional": true,
            "template": "I_11-03",
            "url": "i_11.03.csv"
        }
    }
