        },
        "tFootNotes": {
            "optional": true,
            "template": "FootNotes",
            "url": "FootNotes.csv"
        },
        "tM_20-00": {
            "optional": true,
            "template": "M_20-00",
            "url": "m_20.00.csv"
        }
