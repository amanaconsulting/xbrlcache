      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="TipoInstrumentoRestriccionItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Bonos" />
		    <enumeration value="Efecto de comercio" />
		    <enumeration value="Crédito IF" />
      </restriction>
    </simpleContent>
  </complexType>
 <complexType name="PeriodoItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Anual" />
		    <enumeration value="Semestral" />
		    <enumeration value="Trimestral" />
      </restriction>
    </simpleContent>
  </complexType>
  
  
  
</schema>

