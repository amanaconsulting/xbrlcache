		    <enumeration value="Deudores por operaciones de factoring" />
		    <enumeration value="Contratos de leasing" />
		    <enumeration value="Deudores varios" />
		    <enumeration value="Activos del contrato" />
		    <enumeration value="Cuentas por cobrar por arrendamiento" />
		    <enumeration value="Hipotecas" />
		    <enumeration value="Préstamos a clientes" />
		    <enumeration value="Préstamos a sociedades por acciones" />
		    <enumeration value="Préstamos al gobierno" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="MetodoEvaluacionPerdidasCrediticiasItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Pérdidas crediticias esperadas evaluadas de forma individual" />
		    <enumeration value="Pérdidas crediticias esperadas evaluadas de forma colectiva" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="TipoMedicionPerdidasCrediticiasItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Pérdidas crediticias esperadas durante 12 meses" />
