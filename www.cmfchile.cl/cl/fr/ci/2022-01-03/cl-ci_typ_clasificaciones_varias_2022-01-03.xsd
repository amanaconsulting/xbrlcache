<schema 
	xmlns="http://www.w3.org/2001/XMLSchema" 
	xmlns:cl-ci_typ_cv="http://www.cmfchile.cl/cl/fr/ci/2021-01-04/typ_cv" 
	xmlns:xlink="http://www.w3.org/1999/xlink" 
	xmlns:link="http://www.xbrl.org/2003/linkbase" 
	xmlns:xbrli="http://www.xbrl.org/2003/instance" 
	elementFormDefault="qualified" 
	attributeFormDefault="unqualified" 
	targetNamespace="http://www.cmfchile.cl/cl/fr/ci/2021-01-04/typ_cv">
<import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd" />
  <complexType name="EstadoDelProyectoItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Terminado" />
        <enumeration value="En proceso" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="ActivoGastoItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Activo" />
        <enumeration value="Gasto" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="MedicionItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Mediciones a valor razonable recurrentes" />
		    <enumeration value="Medición a valor razonable no recurrente" />
        <enumeration value="No medidos al valor razonable en el estado de situación financiera sino para los que se revela el valor razonable" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="ActivosItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Valores o títulos de patrimonio para negociar" />
		    <enumeration value="Otros valores o títulos de patrimonio" />
        <enumeration value="Títulos de deuda" />
		    <enumeration value="Inversiones en fondos de cobertura" />
		    <enumeration value="Derivados" />
		    <enumeration value="Propiedades de inversión" />
		    <enumeration value="Activos no corrientes mantenidos para la venta" />
		    <enumeration value="Propiedades, planta y equipos" />
		    <enumeration value="Activos biológicos" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="JerarquiaItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Nivel 1 de la jerarquía del valor razonable" />
		    <enumeration value="Nivel 2 de la jerarquía del valor razonable" />
        <enumeration value="Nivel 3 de la jerarquía del valor razonable" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="AjusteResultadosItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Ingresos de actividades ordinarias" />
        <enumeration value="Costo de ventas" />
        <enumeration value="Otros ingresos" />
        <enumeration value="Costos de distribución" />
        <enumeration value="Gastos de administración" />
        <enumeration value="Otros gastos, por función" />
        <enumeration value="Otras ganancias (pérdidas)" />
        <enumeration value="Ganancias (pérdidas) de actividades operacionales" />
        <enumeration value="Ingresos financieros" />
        <enumeration value="Costos financieros" />
        <enumeration value="Diferencias de cambio" />
        <enumeration value="Resultados por unidades de reajuste" />
        <enumeration value="Otros ingresos" />
        <enumeration value="Disminución (aumento) en inventarios de productos terminados y en proceso" />
        <enumeration value="Materias primas y consumibles utilizados" />
        <enumeration value="Gastos por beneficios a los empleados" />
        <enumeration value="Otros gastos, por naturaleza" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="DividendoItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Provisorio" />
		    <enumeration value="Mínimo obligatorio" />
        <enumeration value="Adicional" />
		    <enumeration value="Eventual" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="TipoEEFFItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Consolidado" />
		    <enumeration value="Individual" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="SINOItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Si" />
		    <enumeration value="No" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="CanalVentaItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Bienes vendidos directamente a los consumidores" />
		    <enumeration value="Bienes vendidos a través de intermediarios" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="CalendarioItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Bienes o servicios transferidos en un momento determinado" />
