    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Clientes del gobierno" />
		    <enumeration value="Clientes que no son del gobierno" />
      </restriction>
    </simpleContent>
  </complexType>
  <complexType name="InstrumentoFinancieroItemType">
    <simpleContent>
      <restriction base="xbrli:tokenItemType">
        <enumeration value="Compromisos de préstamos" />
		    <enumeration value="Contratos Garantía Financiera" />
