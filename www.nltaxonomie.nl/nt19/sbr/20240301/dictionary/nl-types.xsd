  </xs:complexType>
  <xs:complexType id="nl-types_emailItemType" name="emailItemType">
    <xs:simpleContent>
      <xs:restriction base="xbrli:stringItemType">
        <xs:maxLength fixed="false" value="254"/>
        <xs:minLength fixed="false" value="6"/>
        <xs:pattern value="[^@]+@[^@]+"/>
      </xs:restriction>
    </xs:simpleContent>
  </xs:complexType>
