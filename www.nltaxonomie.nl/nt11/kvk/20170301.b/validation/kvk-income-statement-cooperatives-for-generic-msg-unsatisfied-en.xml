    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables5_assertion-unsatisfied-message_EN8" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables5_assertion-unsatisfied-message_EN8" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">AvailableForMemberPayments ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables5_AvailableForMemberPayments}) must be equal to + ResultBeforeMemberPayments ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables5_ResultBeforeMemberPayments}) + ShareInResultsParticipatingInterests ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables5_ShareInResultsParticipatingInterests}).</msg:message>
    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables1_assertion-unsatisfied-message_EN4" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables1_assertion-unsatisfied-message_EN4" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">GrossOperatingResult ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables1_GrossOperatingResult}) must be equal to + NetRevenue ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables1_NetRevenue}) - CostOfSalesNotPurchasedFromMembers ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables1_CostOfSalesNotPurchasedFromMembers}).</msg:message>
    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables3_assertion-unsatisfied-message_EN6" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables3_assertion-unsatisfied-message_EN6" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">NetOperatingResult ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables3_NetOperatingResult}) must be equal to + GrossOperatingResult ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables3_GrossOperatingResult}) - SumOfExpenses ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables3_SumOfExpenses}).</msg:message>
    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_assertion-unsatisfied-message_EN30" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_assertion-unsatisfied-message_EN30" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">OperatingExpenses ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_OperatingExpenses}) must be equal to + CostsRawMaterialsConsumablesNotPurchasedFromMembers ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_CostsRawMaterialsConsumablesNotPurchasedFromMembers}) + CostsOutsourcedWorkOtherExternalExpenses ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_CostsOutsourcedWorkOtherExternalExpenses}) + EmployeeBenefitsExpenses ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_EmployeeBenefitsExpenses}) + DepreciationPropertyPlantEquipmentAmortisationIntangibleAssets ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_DepreciationPropertyPlantEquipmentAmortisationIntangibleAssets}) + ChangesInValueIntangibleAssetsPropertyPlantEquipment ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_ChangesInValueIntangibleAssetsPropertyPlantEquipment}) + ChangesValueInvestmentProperties ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_ChangesValueInvestmentProperties}) + ImpairmentCurrentAssets ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_ImpairmentCurrentAssets}) + OperatingExpensesOther ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateEightVariables1_OperatingExpensesOther}).</msg:message>
    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_assertion-unsatisfied-message_EN1" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_assertion-unsatisfied-message_EN1" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">OperatingIncome ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_OperatingIncome}) must be equal to + NetRevenue ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_NetRevenue}) + ChangesConstructionContracts ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_ChangesConstructionContracts}) + ChangesInventoriesWorkInProgress ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_ChangesInventoriesWorkInProgress}) + CapitalisedProductionCosts ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_CapitalisedProductionCosts}) + OperatingIncomeOther ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateFiveVariables1_OperatingIncomeOther}).</msg:message>
    <msg:message id="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables4_assertion-unsatisfied-message_EN7" xlink:label="ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables4_assertion-unsatisfied-message_EN7" xlink:role="http://www.xbrl.org/2010/role/message" xlink:type="resource" xml:lang="en">ProjectIncome ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables4_ProjectIncome}) must be equal to + NetRevenue ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables4_NetRevenue}) + ChangesConstructionContracts ({$varArc_ConsolidatedIncomeStatementLarge_MsgSeparateTwoVariables4_ChangesConstructionContracts}).</msg:message>
