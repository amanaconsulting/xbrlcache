                   xlink:type="resource"
                   xml:lang="es">{fmt:contenido-comun-celdas(($celda0306,$celda0345,$celda0346))}{ if ($esEntidadAutorizada) then ('El importe de las claves tiene que cumplir las siguientes reglas: ', if (some $i in (2009,2010,2011,2012,2013,2014,2015) satisfies $anno eq $i) then ('/n/t',fmt:valor(($celda0306),($celda0306)), '[c0306e3010]', fmt:operador('ge'), fmt:valor(($operacion0306),($operacion0306)), '[c0345e3010 c0108e3010 c0109e3010 c0112e3010 c0150e3010 c0151e3010 c0205e3010 c0445e3010 c0275e3010 c0295e3010 c0308e3010] y' ) else '' ,'/n/t', fmt:valor(($celda0345),($celda0345)), '[c0345e3010]' , fmt:operador('gt'), ' 0 y', (if ($anno eq 2008) then ('/n/t',fmt:valor(($celda0346),($celda0346)), '[c0346e3010]' , fmt:operador('gt'), '0.9') else ('/n/t',fmt:valor(($celda0346),($celda0346)), '[c0346e3010]' , fmt:operador('eq'), '0.8')) ) else ('Las claves 0306, 0345 y 0346 no pueden tener importe: (' , fmt:valor(($celda0306),($celda0306)), '[c0306e3010]' , fmt:operador('eq'), fmt:valor((0),(0)), ') y (' , fmt:valor(($celda0345),($celda0345)), '[c0345e3010]' , fmt:operador('eq'), fmt:valor((0),(0)) , ') y (' , fmt:valor(($celda0346),($celda0346)), '[c0346e3010]' , fmt:operador('eq'), fmt:valor((0),(0)) , ')') }</msg:message>
