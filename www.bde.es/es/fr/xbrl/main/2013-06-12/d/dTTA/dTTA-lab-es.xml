    <link:labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="loc_estr_x8" xlink:to="label_estr_x8" />
    <link:label xlink:type="resource" xlink:label="label_estr_x8_1" xml:lang="es" xlink:role="http://www.xbrl.org/2003/role/verboseLabel">Informes realizados por internet en los que la valoración del bien se facilita en tiempo real.</link:label>
