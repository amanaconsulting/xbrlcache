<link:linkbase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:link="http://www.xbrl.org/2003/linkbase" xmlns:gen="http://xbrl.org/2008/generic" xmlns:label="http://xbrl.org/2008/label" xmlns:variable="http://xbrl.org/2008/variable" xsi:schemaLocation="http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd http://xbrl.org/2008/generic http://www.xbrl.org/2008/generic-link.xsd http://xbrl.org/2008/label http://www.xbrl.org/2008/generic-label.xsd http://xbrl.org/2008/variable http://www.xbrl.org/2008/variable.xsd">
  <gen:link xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
    <variable:precondition xlink:type="resource" xlink:label="es_condUEM" test="($entitySize= 'GRANDE') or ($entitySize= 'PEQUEÑA' and month-from-date($refPeriodEndDate) = (3,6,9,12)) " id="es_condUEM" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadConSucExt" test="$entidadSucursalExt = true()" id="es_entidadConSucExt" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadGrande" test="$entitySize = 'GRANDE'" id="es_entidadGrande" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadSinSucExt" test="$entidadSucursalExt = false()" id="es_entidadSinSucExt" />
    <variable:precondition xlink:type="resource" xlink:label="es_p1" test="month-from-date($refPeriodEndDate) = (6,9,12)" id="es_p1" />
    <variable:precondition xlink:type="resource" xlink:label="es_tipoEntidadcc-ca" test="($TipoEntidad = 'CC') or ($TipoEntidad = 'CA')" id="es_tipoEntidadcc-ca" />
    <variable:precondition xlink:type="resource" xlink:label="es_tipoEntidadNOcc-ca" test="($TipoEntidad !='CC') and ($TipoEntidad !='CA')" id="es_tipoEntidadNOcc-ca" />
    <variable:precondition xlink:type="resource" xlink:label="es_trimestre" test="month-from-date($refPeriodEndDate) = (3,6,9,12)" id="es_trimestre" />
  </gen:link>
</link:linkbase>