<link:linkbase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:link="http://www.xbrl.org/2003/linkbase" xmlns:gen="http://xbrl.org/2008/generic" xmlns:label="http://xbrl.org/2008/label" xmlns:variable="http://xbrl.org/2008/variable" xsi:schemaLocation="http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd http://xbrl.org/2008/generic http://www.xbrl.org/2008/generic-link.xsd http://xbrl.org/2008/label http://www.xbrl.org/2008/generic-label.xsd http://xbrl.org/2008/variable http://www.xbrl.org/2008/variable.xsd">
  <gen:link xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
    <variable:precondition xlink:type="resource" xlink:label="es_condUEM" test="($entitySize= 'GRANDE') or ($entitySize= 'PEQUEÑA' and month-from-date($refPeriodEndDate) = (3,6,9,12)) " id="es_condUEM" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadConSucExt" test="$entidadSucursalExt = true()" id="es_entidadConSucExt" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadGrande" test="$entitySize = 'GRANDE'" id="es_entidadGrande" />
    <variable:precondition xlink:type="resource" xlink:label="es_entidadSinSucExt" test="$entidadSucursalExt = false()" id="es_entidadSinSucExt" />
    <variable:precondition xlink:type="resource" xlink:label="es_mesDiciembre" test="month-from-date($refPeriodEndDate) = 12" id="es_mesDiciembre" />
    <variable:precondition xlink:type="resource" xlink:label="es_mesesJunSeptDic" test="month-from-date($refPeriodEndDate) = (6,9,12)" id="es_mesesJunSeptDic" />
    <variable:precondition xlink:type="resource" xlink:label="es_mesMarzo" test="month-from-date($refPeriodEndDate) = 3" id="es_mesMarzo" />
    <variable:precondition xlink:type="resource" xlink:label="es_noSucExtranjera" test="$sucExtranjera != 'SECE' and $sucExtranjera != 'SECC'" id="es_noSucExtranjera" />
    <variable:precondition xlink:type="resource" xlink:label="es_sucExtranjera" test="$sucExtranjera = 'SECE' or $sucExtranjera = 'SECC'" id="es_sucExtranjera" />
    <variable:precondition xlink:type="resource" xlink:label="es_tipoEntidadcc-ca" test="$tipoEntidad = 'CC' or $tipoEntidad = 'CA'" id="es_tipoEntidadcc-ca" />
    <variable:precondition xlink:type="resource" xlink:label="es_tipoEntidadNOcc-ca" test="$tipoEntidad != 'CC' or $tipoEntidad != 'CA'" id="es_tipoEntidadNOcc-ca" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR1" test="$TR - 0.01 &gt; 0.007500" id="es_tramoTR1" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR2" test="$TR - 0.01 &lt;= 0.007500 and 0.007500 &lt; $TR - 0.005" id="es_tramoTR2" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR3" test="$TR - 0.005 &lt;= 0.007500 and 0.007500 &lt; $TR" id="es_tramoTR3" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR4" test="$TR &lt;= 0.007500 and 0.007500 &lt; $TR + 0.005" id="es_tramoTR4" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR5" test="$TR + 0.005 &lt;= 0.007500 and 0.007500 &lt; $TR + 0.01" id="es_tramoTR5" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR6" test="$TR - 0.01 &lt; 0" id="es_tramoTR6" />
    <variable:precondition xlink:type="resource" xlink:label="es_tramoTR7" test="$TR - 0.005 &lt; 0" id="es_tramoTR7" />
    <variable:precondition xlink:type="resource" xlink:label="es_trimestre" test="month-from-date($refPeriodEndDate) = (3,6,9,12)" id="es_trimestre" />
  </gen:link>
</link:linkbase>