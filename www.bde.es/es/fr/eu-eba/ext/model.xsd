<schema targetNamespace="http://www.eba.europa.eu/xbrl/ext/model"
	xmlns:link="http://www.xbrl.org/2003/linkbase"
	xmlns:xl="http://www.xbrl.org/2003/XLink"
	xmlns:xbrli="http://www.xbrl.org/2003/instance"
	xmlns:gen="http://xbrl.org/2008/generic"
	xmlns:model="http://www.eba.europa.eu/xbrl/ext/model"
	xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<annotation>
		<documentation>Schema for definitions of the model</documentation>

		<appinfo>
			<!-- 
				Calculation arcs between members to establish calculation relationships
				in the context of a hierarchy
			-->
			<link:arcroleType id="complete-breakdown" cyclesAllowed="undirected"
				arcroleURI="http://www.eba.europa.eu/xbrl/arcrole/complete-breakdown">
				<link:definition>Member is equal to the aggregation of its breakdown</link:definition>
				<link:usedOn>link:calculationArc</link:usedOn>
			</link:arcroleType>
			
			<link:arcroleType id="partial-breakdown" cyclesAllowed="undirected"
				arcroleURI="http://www.eba.europa.eu/xbrl/arcrole/partial-breakdown">
				<link:definition>Member is equal or greater than the aggregation of its breakdown</link:definition>
				<link:usedOn>link:calculationArc</link:usedOn>
			</link:arcroleType>
			
			<link:arcroleType id="superset-breakdown" cyclesAllowed="undirected"
				arcroleURI="http://www.eba.europa.eu/xbrl/arcrole/superset-breakdown">
				<link:definition>Member is equal or less than the aggregation of its breakdown</link:definition>
				<link:usedOn>link:calculationArc</link:usedOn>
			</link:arcroleType>
			
			<!-- Role used to link the objects of the model with additional properties -->
			<link:arcroleType id="model-properties" cyclesAllowed="undirected"
				arcroleURI="http://www.eba.europa.eu/xbrl/arcrole/model-properties">
				<link:definition>Model object is linked to additional information resources</link:definition>
				<link:usedOn>gen:arc</link:usedOn>				
			</link:arcroleType>			
			
			<!-- Role used to establish the presentation hierarchy between table groups and tables -->
			<link:arcroleType id="group-table" cyclesAllowed="undirected"
				arcroleURI="http://www.eba.europa.eu/xbrl/arcrole/group-table">
				<link:definition>Table group is the parent of other table groups and/or other tables</link:definition>
				<link:usedOn>gen:arc</link:usedOn>				
			</link:arcroleType>			
						
		</appinfo>
	</annotation>
	
	<import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
	<import namespace="http://www.xbrl.org/2003/XLink" schemaLocation="http://www.xbrl.org/2003/xl-2003-12-31.xsd"/>
	
	<!-- Types -->
	<complexType name="explicitDomainType" id="explicitDomainType" abstract="true">
		<annotation>
			<documentation xml:lang="en">The explicit domain type is used for elements that
				represent the set of elements in an explicit domain. These elements are not domain
				items and thus, cannot appear in instance documents. Domain items are represented
				using the standard type defined in the type registry. </documentation>
		</annotation>
		<simpleContent>
			<restriction base="xbrli:stringItemType">
				<length fixed="true" value="0" />
			</restriction>
		</simpleContent>
	</complexType>

	<complexType name="familyType" id="familyType" abstract="true">
		<annotation>
			<documentation xml:lang="en">TODO</documentation>
		</annotation>
		<simpleContent>
			<restriction base="xbrli:stringItemType">
				<length fixed="true" value="0" />
			</restriction>
		</simpleContent>
	</complexType>

	<complexType name="frameworkType" id="frameworkType" abstract="true">
		<annotation>
			<documentation xml:lang="en">TODO</documentation>
		</annotation>
		<simpleContent>
			<restriction base="xbrli:stringItemType">
				<length fixed="true" value="0" />
			</restriction>
		</simpleContent>
	</complexType>

	<complexType name="taxonomyType" id="taxonomyType" abstract="true">
		<annotation>
			<documentation xml:lang="en">TODO</documentation>
		</annotation>
		<simpleContent>
			<restriction base="xbrli:stringItemType">
				<length fixed="true" value="0" />
			</restriction>
		</simpleContent>
	</complexType>		

	<complexType name="tableGroupType" id="tableGroupType" abstract="true">
		<annotation>
			<documentation xml:lang="en">TODO</documentation>
		</annotation>	
		<simpleContent>
			<restriction base="xbrli:stringItemType">
				<length fixed="true" value="0" />
			</restriction>
		</simpleContent>
	</complexType>	
	

	<!-- Attributes -->

	<attribute name="creationDate" type="date">
		<annotation>
			<documentation>Attribute to provide information about the time of creation of an
				element</documentation>
		</annotation>
	</attribute>
	
	<attribute name="modificationDate" type="date">
		<annotation>
			<documentation>Attribute to provide information about the last time when an element was
				modified. If not included, the element has not been modified since it was
				created</documentation>
		</annotation>
	</attribute>
	
	<attribute name="fromDate" type="date">
		<annotation>
			<documentation>Establish the start date of the validity period of an element</documentation>
		</annotation>
	</attribute>
	
	<attribute name="toDate" type="date">
		<annotation>
			<documentation>Establish the end date of the validity period of an element</documentation>
		</annotation>
	</attribute>	

	<attribute name="isDefaultMember" type="boolean" default="false">
		<annotation>
			<documentation>Indicates wether an explicit domain member is the explicit member of its
				domain</documentation>
		</annotation>
	</attribute>
	
	<attribute name="periodType" default="instant">
		<annotation>
			<documentation>Indicates wether a data point applies is referred to an instant or an interval of time</documentation>
		</annotation>
		<simpleType>
			<restriction base="token">
				<enumeration value="instant" />
				<enumeration value="duration" />
			</restriction>
		</simpleType>
	</attribute>
	
	<attribute name="domain" type="QName">
		<annotation>
			<documentation>Used in base items whose set of values is that of an existing domain. It represents the QName of the domain</documentation>
		</annotation>		
	</attribute>
	
	<!-- Attribute groups -->
	
	<attributeGroup name="publicElementAttrs" id="publicElementAttrs">
		<attribute ref="model:creationDate"/>
		<attribute ref="model:modificationDate"/>					
	</attributeGroup>

	<attributeGroup name="dictionaryElementAttrs">
		<attributeGroup ref="model:publicElementAttrs"/>
		<attribute ref="model:fromDate"/>
		<attribute ref="model:toDate"/>
	</attributeGroup>
	
	
	<!-- Resources -->
	<!-- Not necessary for the moment.
	<complexType name="resource.type">
		<complexContent mixed="true">
			<extension base="xl:resourceType">
				<anyAttribute namespace="http://www.w3.org/XML/1998/namespace" processContents="lax"/>
			</extension>
		</complexContent>
	</complexType>
	
	<element name="resource" abstract="true" substitutionGroup="xl:resource" type="model:resource.type"/>
	-->
</schema>
